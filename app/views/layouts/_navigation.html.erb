<%= link_to "Chat room", root_path, class: "navbar-brand item"%>
<a class="item">
  Messages
</a>
<div class="right menu">
  <div class="ui dropdown item">
    Acount <i class="dropdown icon"></i>
    <div class="menu">
      <% if logged_in? %>
        <%= link_to "Log out", logout_path, method: :delete, class: "item" %>
      <% else %>
        <%= link_to "Log in", login_path, class: "navbar-brand item" %>
        <a class="item">Sign up</a>
      <% end %>
    </div>
  </div>
  <% if !logged_in? %>
    <div class="item">
        <%= link_to "Log in", login_path, class: "ui primary button" %>
    </div>
  <% end %>
  
</div>
